@page "/Admindeletesupplier"
<h2>Admin Delete Supplier</h2>
<table class="table">
	<thead>
		<tr>
			<th>Supplier Name</th>
			<th>Last Name</th>
			<th>Gender</th>
			<th>DOB</th>
		</tr>
	</thead>
	<tbody>

		@foreach (Supplier s in DatabaseInstance.viewSuppliers())
		{
			<tr>
				<td>@s.name</td>
			</tr>
		}
	</tbody>
</table>
<button class="btn btn-primary">Confirm Delete</button>
@code {
	[Parameter]
	public int? AuthorID { get; set; }
}
