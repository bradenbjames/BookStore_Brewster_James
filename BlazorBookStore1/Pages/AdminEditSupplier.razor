@page "/Admineditsupplier/{supplierID:int}"
<h2>Admin Delete Supplier</h2>
<table class="table table-striped">
	<thead>
		<tr>
			<th scope="col">Supplier Name</th>
		</tr>
	</thead>
	<tbody>

		@foreach (Supplier s in DatabaseInstance.viewSuppliers())
		{
			<tr>
				<td>@s.name</td>
			</tr>
		}
	</tbody>
</table>
<button class="btn btn-primary">Confirm Changes</button>
<button class="btn btn-primary">Delete Supplier</button>
@code {
	[Parameter]
	public int? supplierID { get; set; }
}
